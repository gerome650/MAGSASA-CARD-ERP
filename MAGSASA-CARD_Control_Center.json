  "databases": {
    "ci_intelligence_reports": {
      "title": "CI Intelligence Reports",
      "description": "Weekly CI intelligence reports with failure analysis and auto-fix metrics",
      "icon": "üìä",
      "properties": {
        "Name": {
          "type": "title",
          "description": "Weekly report filename"
        },
        "Week Of": {
          "type": "date",
          "description": "Report generation date"
        },
        "Analysis Period": {
          "type": "number",
          "format": "number",
          "description": "Days analyzed"
        },
        "Total Failures": {
          "type": "number",
          "format": "number",
          "description": "Total failures in period"
        },
        "Auto-Fix Success Rate": {
          "type": "number",
          "format": "percent",
          "description": "Percentage of successful auto-fixes"
        },
        "Avg MTTR (minutes)": {
          "type": "number",
          "format": "number",
          "description": "Mean Time to Recovery"
        },
        "Total Runs": {
          "type": "number",
          "format": "number",
          "description": "Total CI runs in period",
          "required": false
        },
        "Failed Runs": {
          "type": "number",
          "format": "number",
          "description": "Failed CI runs in period",
          "required": false
        },
        "Top Failures": {
          "type": "rich_text",
          "description": "Most common failure type"
        },
        "Recommendations": {
          "type": "rich_text",
          "description": "Actionable recommendations"
        },
        "Status": {
          "type": "select",
          "options": [
            {"name": "Active", "color": "green"},
            {"name": "Archived", "color": "gray"},
            {"name": "Under Review", "color": "yellow"}
          ],
          "description": "Report status"
        },
        "Type": {
          "type": "select",
          "options": [
            {"name": "Weekly", "color": "blue"},
            {"name": "Monthly", "color": "purple"},
            {"name": "Quarterly", "color": "orange"},
            {"name": "Ad-hoc", "color": "red"}
          ],
          "description": "Report type"
        },
        "GitHub Link": {
          "type": "url",
          "description": "Link to GitHub workflow"
        },
        "Created": {
          "type": "created_time",
          "description": "Creation timestamp"
        },
        "Last Modified": {
          "type": "last_edited_time",
          "description": "Last modification timestamp"
        }
      },
      "sample_data": [
        {
          "Name": "Weekly CI Report - Week 50, 2024",
