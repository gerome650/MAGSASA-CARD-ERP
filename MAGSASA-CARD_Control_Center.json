        "Latest CI Health": {
          "type": "relation",
          "related_database": "ci_intelligence_reports",
          "description": "Links to latest CI report"
        },
        "Related PR": {
          "type": "url",
          "description": "Link to primary PR"
        },
        "Workflow Pass Rate": {
          "type": "number",
          "format": "percent",
          "description": "Percentage of workflows passing"
        },
        "Auto-Fix Success": {
          "type": "number",
          "format": "percent",
          "description": "Percentage of successful auto-fixes"
        },
        "MTTR": {
          "type": "number",
          "format": "number",
          "description": "Mean Time to Recovery"
        },
        "Trend": {
          "type": "select",
          "options": [
            {"name": "Improving", "color": "green"},
            {"name": "Stable", "color": "blue"},
            {"name": "Degrading", "color": "red"}
