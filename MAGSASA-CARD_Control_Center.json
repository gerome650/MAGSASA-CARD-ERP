        "Risk Level": {
          "type": "select",
          "options": [
            {"name": "Low", "color": "green"},
            {"name": "Medium", "color": "yellow"},
            {"name": "High", "color": "red"},
            {"name": "Critical", "color": "red"}
          ],
          "description": "Risk assessment"
