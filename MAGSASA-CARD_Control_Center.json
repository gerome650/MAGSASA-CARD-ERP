  "views": {
    "kanban_by_stage": {
      "database": "engineering_roadmap",
      "type": "kanban",
      "title": "Kanban by Stage",
      "group_by": "Status",
      "sort": [
        {
          "property": "Target Date",
          "direction": "ascending"
        }
      ]
    },
    "timeline_view": {
      "database": "engineering_roadmap",
      "type": "timeline",
      "title": "Timeline View",
      "group_by": "Status",
      "date_range": {
        "start": "Target Date",
        "end": null
      }
    },
    "kpi_trends": {
      "database": "kpi_dashboard",
      "type": "table",
      "title": "KPI Trends",
      "sort": [
        {
          "property": "Category",
          "direction": "ascending"
        },
        {
          "property": "Current Value",
          "direction": "descending"
        }
      ],
      "filter": {
        "property": "Status",
        "select": {
          "does_not_equal": "Archived"
        }
      }
    },
    "weekly_reports": {
      "database": "ci_intelligence_reports",
      "type": "table",
      "title": "Weekly Reports",
      "sort": [
        {
          "property": "Week Of",
          "direction": "descending"
        }
      ],
      "filter": {
        "property": "Type",
        "select": {
          "equals": "Weekly"
        }
      }
    }
  },
  "environment_variables": {
